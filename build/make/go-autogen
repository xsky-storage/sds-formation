#!/bin/bash

rm -rf autogen

mkdir -p autogen/version
cat > autogen/version/version.go <<DVEOF
// AUTOGENERATED FILE; see $BASH_SOURCE
package version

var (
	GITCOMMIT string = "$GITCOMMIT"
	VERSION   string = "$VERSION"
	BUILDTIME string = "$BUILDTIME"

	IAMSTATIC string = "${IAMSTATIC:-true}"
)
DVEOF
